{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.py" },
    { "src": "/static/styles.css", "dest": "/frontend/styles.css" },
    { "src": "/static/app.js", "dest": "/frontend/app.js" },
    { "src": "/", "dest": "/frontend/index.html" }
  ]
}
